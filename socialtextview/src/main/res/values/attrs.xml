<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SocialTextView">
        <attr name="android:text"/>
        <attr name="normalTextColor"/>
        <attr name="underLine"/>
        <attr name="hashtagColor"/>
        <attr name="mentionColor"/>
        <attr name="phoneColor"/>
        <attr name="urlColor"/>
        <attr name="emailColor"/>
        <attr name="selectedColor"/>
        <attr name="linkType" format="flags">
            <flag name="hashtag" value="2"/>
            <flag name="mention" value="4"/>
            <flag name="url" value="8"/>
            <flag name="email" value="16"/>
            <flag name="phone" value="32"/>
        </attr>
    </declare-styleable>
</resources>